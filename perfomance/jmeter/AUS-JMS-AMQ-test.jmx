            &quot;   &lt;updateType&gt;BACKORDER&lt;/updateType&gt;\n&quot; +
            &quot;   &lt;gatewayOrderNumber&gt;&quot;+orderId+&quot;-AQ-8656-1-&quot;+randomId+&quot;&lt;/gatewayOrderNumber&gt;\n&quot; +
            &quot;   &lt;orderLines&gt;\n&quot; +
            &quot;      &lt;lineNumber&gt;1&lt;/lineNumber&gt;\n&quot; +
            &quot;      &lt;backorderInfo /&gt;\n&quot; +
            &quot;      &lt;attributes&gt;\n&quot; +
            &quot;         &lt;name&gt;ShipsBy&lt;/name&gt;\n&quot; +
            &quot;         &lt;value&gt;5â€“7 business days&lt;/value&gt;\n&quot; +
            &quot;      &lt;/attributes&gt;\n&quot; +
            &quot;      &lt;attributes&gt;\n&quot; +
            &quot;         &lt;name&gt;DeliversBy&lt;/name&gt;\n&quot; +
            &quot;         &lt;value /&gt;\n&quot; +
            &quot;      &lt;/attributes&gt;\n&quot; +
            &quot;   &lt;/orderLines&gt;\n&quot; +
            &quot;&lt;/ns2:PlatformUpdateMessage&gt;&quot;;
vars.put(&quot;MQ_CONTENT&quot;,expectedMessage);</stringProp>
