{
  "name": "app-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node imageserver.js | ng serve",
    "serve-image": "node imageserver.js",
    "serve": "node imageserver.js | cross-env ENV=local443 ng serve",
    "build": "ng build --configuration production",
    "test": "ng lint && ng test --code-coverage",
    "proxy:local443": "node imageserver.js | cross-env ENV=local443 ng serve",
    "proxy:local8443": "node imageserver.js | cross-env ENV=local8443 ng serve",
    "proxy:ang-dev": "node imageserver.js | cross-env ENV=ang-dev ng serve",
    "proxy:ang-qa": "node imageserver.js | cross-env ENV=ang-qa ng serve",
    "proxy:capbqa": "node imageserver.js | cross-env ENV=capbqa ng serve",
    "proxy:dev": "node imageserver.js | cross-env ENV=dev ng serve",
    "proxy:qa1": "node imageserver.js | cross-env ENV=qa1 ng serve",
    "proxy:qa2": "node imageserver.js | cross-env ENV=qa2 ng serve",
    "proxy:uat": "node imageserver.js | cross-env ENV=uat ng serve",
    "build-map": "ng build --configuration production --baseHref=/ui/ --source-map",
    "explore": "source-map-explorer dist/**/*.js",
    "iframe": "node dev/iframe.js",
    "lint": "ng lint",
    "lintf": "ng lint --fix",
    "e2e": "ng e2e",
    "sonar": "node sonar-scan.js --publish",
    "no-sonar": "node sonar-scan.js --preview",
    "template-banners-build": "sass --no-source-map src/app/modules/slideshow/template-banner/template-banner.component.scss src/styles/template-banners.css"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
